create table product (
	pro_code int not null AUTO_INCREMENT,
	pro_nm NVARCHAR(20) not null,
	menu_code VARCHAR(7) not null,
	pro_company NVARCHAR(20) not null,
	pro_img VARCHAR(20) not null,
	srch_code1 int not null,
	srch_code2 int,
	srch_code3 int,
	srch_nm1 NVARCHAR(20) not null,
	srch_nm2 NVARCHAR(20),
	srch_nm3 NVARCHAR(20),
	create_dt DATETIME default CURRENT_TIMESTAMP,
	create_id int not null,
	dummy1 VARCHAR(20),
	dummy2 VARCHAR(20),
	dummy3 VARCHAR(20),
	PRIMARY KEY(pro_code)
)
desc product

drop table product

delete from product

