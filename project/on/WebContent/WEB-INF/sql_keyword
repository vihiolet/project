create table keyword (
	srch_code int not null AUTO_INCREMENT,
	srch_name VARCHAR(20) not null,
	create_dt DATETIME default CURRENT_TIMESTAMP,
	create_id int not null,
	dumminy1 VARCHAR(20),
	dumminy2 VARCHAR(20),
	dumminy3 VARCHAR(20),
	PRIMARY KEY(srch_code)
)
desc keyword

drop table keyword

insert into keyword(srch_name, create_id) values ('새활용', 10)

select * from keyword 