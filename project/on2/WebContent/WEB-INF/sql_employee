create table employee (
	emp_code int not null AUTO_INCREMENT,
	emp_nm NVARCHAR(20) not null,	
	password int not null,	
	create_dt DATETIME default CURRENT_TIMESTAMP,
	create_id int not null,
	dummy1 VARCHAR(20),
	dummy2 VARCHAR(20),
	dummy3 VARCHAR(20),
	PRIMARY KEY(emp_code)
)
desc employee

drop table employee

